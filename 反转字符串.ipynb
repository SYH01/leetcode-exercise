    "class Solution:\n",
    "    def reverseString(self, s: List[str]) -> None:\n",
    "        left,right=0,len(s)-1;\n",
    "        while left<right :\n",
    "            temp=s[left];\n",
    "            s[left]=s[right];\n",
    "            s[right]=temp;\n",
    "            left+=1;\n",
    "            right-=1;\n"
   ]
  }
